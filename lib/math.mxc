// 累乗, 冪根, 絶対値
// pow(), sqrt(), cbrt(), hypot(), abs()
fn pow(x: int, n: int): int {
    let result = 1;
    while n > 1 {
        let {
            n2 = n / 2;
            i = n - (2 * n2);
        }
        if i > 0 {
            result = result * x;
        }
        x = x * x;
        n = n2;
    }
    return result * x;
}

fn sqrt(x :float): float {
    if x <= 0.0 {
        return 0.0;
    }
    let {
        before = x * 0.5;
        after = (before + x / before) * 0.5;
        d = before - after;
    }
    if d < 0.0 {
        d = -d;
    }
    while d > 0.000000001 {
        before = after;
        after = (before + x / before) * 0.5;
        d = before - after;
    }
    return after;
}

fn abs(i: int) = if i >= 0 i else -i;


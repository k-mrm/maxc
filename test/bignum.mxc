def test_big_add() {
    let a = 891749832710498174827472 + 4783271840721421783974921;
    let b = 428146327145219647214879 + -28146327145219647214879;
    let c = -28146327145219647214879 + 428146327145219647214879; 
    let d = -891749832710498174827472 + -4783271840721421783974921;

    assert a == 5675021673431919958802393;
    assert b == 400000000000000000000000;
    assert c == 400000000000000000000000;
    assert d == -5675021673431919958802393;
}

def test_big_sub() {
    let a = 428146327145219647214879 - 28146327145219647214879;
    let b = -891749832710498174827472 - 4783271840721421783974921;
    let c = 891749832710498174827472 - -4783271840721421783974921;
    let d = -28146327145219647214879 - -428146327145219647214879;

    assert a == 400000000000000000000000;
    assert b == -5675021673431919958802393;
    assert c == 5675021673431919958802393;
    assert d == 400000000000000000000000;
}

def test_big_mul() {
    let a = 73472148014787328174087081784 * 78943178973516587687531;
    let b = -73472148014787328174087081784 * 78943178973516587687531;
    let c = 73472148014787328174087081784 * -78943178973516587687531;
    let d = -73472148014787328174087081784 * -78943178973516587687531;

    assert a == 5800124930300057505620336136415813778007816634035304; 
    assert b == -5800124930300057505620336136415813778007816634035304;
    assert c == -5800124930300057505620336136415813778007816634035304;
    assert d == 5800124930300057505620336136415813778007816634035304;
}

test_big_add();
test_big_sub();
test_big_mul();
